/* CSS Document */
@import url('normalize.css');

html
{
    font-family: sans-serif; /* 3 */

    color: #000; /* 2 */
    background: #ccc; /* 1 */

        -ms-text-size-adjust: 100%; /* 4 */
    -webkit-text-size-adjust: 100%; /* 4 */
}

body
{
    overflow: auto;

    width: 100%;
    height: 100%;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

#container
{
    z-index: 101;

    width: 700px;
    height: 500px;
    margin-top: 20px;
    margin-right: auto;
    margin-left: auto;
    padding: 6px;

    -webkit-border-radius: 6px;
       -moz-border-radius: 6px;
            border-radius: 6px;
    background: rgba(13,79,139,.7);
}

#heading
{
    font-size: 14px;
    font-weight: bold;
    line-height: 44px;

    clear: right;

    width: 700px;
    height: 44px;
    margin: 0;
    padding: 0;

    text-align: center;

    color: #444;
    border-bottom: 1px solid #bababa;
    -webkit-border-radius: 4px 4px 0 0;
       -moz-border-radius: 4px 4px 0 0;
            border-radius: 4px 4px 0 0;
    background-image: -webkit-linear-gradient(top, rgb(249, 249, 249), rgb(233, 233, 233));
    background-image:    -moz-linear-gradient(top, rgb(249, 249, 249), rgb(233, 233, 233));
    background-image:      -o-linear-gradient(top, rgb(249, 249, 249), rgb(233, 233, 233));
    background-image:     -ms-linear-gradient(top, rgb(249, 249, 249), rgb(233, 233, 233));
    background-image:         linear-gradient(top, rgb(249, 249, 249), rgb(233, 233, 233));
    -webkit-box-shadow: inset 0 -1px 0 #fff,  0 1px 3px rgba(0,0,0,.08);
       -moz-box-shadow: inset 0 -1px 0 #fff,  0 1px 3px rgba(0,0,0,.08);
            box-shadow: inset 0 -1px 0 #fff,  0 1px 3px rgba(0,0,0,.08);
    text-shadow: 0 1px 0 #fff;

    filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0, StartColorStr='#f9f9f9', EndColorStr='#e9e9e9');
}
#content
{
    position: relative;

    width: 700px;
    height: 355px;
    padding-top: 1px;

    -webkit-border-radius: 0 0 4px 4px;
       -moz-border-radius: 0 0 4px 4px;
            border-radius: 0 0 4px 4px;
    background: #fcfcfc;
    -webkit-box-shadow: 0 1px 3px rgba(0,0,0,.25);
       -moz-box-shadow: 0 1px 3px rgba(0,0,0,.25);
            box-shadow: 0 1px 3px rgba(0,0,0,.25);
}
#oooffice
{
	position: relative;
	width: 505px;
	margin-top: 15px;
	margin-right: auto;
	margin-left: auto;
	padding: 5px;
	border: 8px solid rgba(13,79,139,.4);
	background-color: rgba(186,188,199,0.7);
}

.error
{
    font-size: xx-small;
    font-style: italic;

    color: red;
}
#container #heading .head
{
    float: left;
    clear: right;

    margin-left: 100px;
}



#container #heading #right
{
    font-family: 'Lucida Sans Unicode', 'Lucida Grande', sans-serif;
    font-size: x-small;

    margin: 0;
    padding: 0;

    text-align: right;
}
#container #heading #right p
{
    margin-top: 0;
    margin-right: 50px;
    margin-bottom: 0;
    margin-left: 0;
    padding: 0;
}

#container #footer
{
    width: 697px;
    height: 90px;
    margin-top: 5px;
    margin-right: auto;
    margin-left: auto;

    border: medium ridge #09f;
    -webkit-border-radius: 6px;
       -moz-border-radius: 6px;
            border-radius: 6px;
    background: #ccc;
}
#footer .r_message
{
    font-family: 'Courier New', Courier, monospace;
    font-size: x-small;

    width: 691px;
    height: 85px;

    resize: none;
}
#txtAreaMessage
{
    font-family: Calibri, Arial;
    font-size: small;
    font-style: normal;

    width: 100%;

    resize: none;
}

.vac
{
    font-family: Calibri, Arial;
    font-size: small;
    font-weight: normal;

    width: 150px;

    text-align: left;
}

.in
{
    font-family: Calibri, Arial;
    font-size: small;
    font-style: normal;

    height: 22px;
}

.v_disable,
.v_enable
{
    font-family: Calibri, Arial;
    font-size: small;

    margin: 0;
    padding: 0;

    vertical-align: middle;
}
#enable
{
    margin: 0;
    padding: 0;

    vertical-align: middle;
}

#submit
{
    vertical-align: middle;
}

/* http://www.bestcssbuttongenerator.com/ */
.myButton {
    margin-top: 4px;
    background-color:rgba(13,79,139,.7);
    border:1px solid #29668f;
    display:inline-block;
    cursor:pointer;
    color:#ffffff;
    font-family:Verdana;
    font-size:12px;
    padding:4px 13px;
    text-decoration:none;
    text-shadow:0px 1px 0px #3d768a;
}
.myButton:hover {
    background-color:#408c99;
}
.myButton:active {
    position:relative;
    top:1px;
}
.myButton:disabled {
    background-color:#ccc;
    color: #aaa;
    pointer-events: none;
}


/* onoffswitch */
/* http://proto.io/freebies/onoff/ */
.onoffswitch
{
    position: relative;

    width: 60px;

    -webkit-user-select: none;
       -moz-user-select: none;
        -ms-user-select: none;
}
.onoffswitch-checkbox
{
    display: none;
}
.onoffswitch-label
{
    display: block;
    overflow: hidden;

    cursor: pointer;

    border: 2px solid #999;
    border-radius: 20px;
}
.onoffswitch-inner
{
    width: 200%;
    margin-left: -100%;

    -webkit-transition: margin .3s ease-in 0s;
       -moz-transition: margin .3s ease-in 0s;
         -o-transition: margin .3s ease-in 0s;
            transition: margin .3s ease-in 0s;
}
.onoffswitch-inner:before, .onoffswitch-inner:after
{
    font-family: Trebuchet, Arial, sans-serif;
    font-size: 14px;
    font-weight: bold;
    line-height: 5px;

    float: left;

    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;
    width: 50%;
    height: 5px;
    padding: 0;

    color: white;
}
.onoffswitch-inner:before
{
    padding-left: 10px;

    content: '';

    color: #fff;
    background-color: rgba(13,79,139,.7);
}
.onoffswitch-inner:after
{
    padding-right: 10px;

    content: '';
    text-align: right;

    color: #999;
    background-color: #eee;
}
.onoffswitch-switch
{
    position: absolute;
    top: 0;
    right: 51px;
    bottom: 0;

    width: 15px;
    margin: -5px;

    -webkit-transition: all .3s ease-in 0s;
       -moz-transition: all .3s ease-in 0s;
         -o-transition: all .3s ease-in 0s;
            transition: all .3s ease-in 0s;

    border: 2px solid #999;
    /*border-radius: 20px;*/
    background: #fff;
}
.onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-inner
{
    margin-left: 0;
}
.onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-switch
{
    right: 0;
}

.on_off {
    font-family:Verdana;
    font-size:12px;
}
